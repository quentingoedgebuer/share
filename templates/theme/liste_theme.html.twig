</div>        


{% for theme in themes %}
<div class="row justify-content-center">           
<div class="col-8 bg-black p-4 m-0 text-primary">

<table class="table">
  <thead>
  
      <th scope="col">{{theme.libelle}}</th>
   
  </thead>          
           
             
</div>
</div>

 {% endfor %}

{% endblock %}
